  "source": "// SPDX-License-Identifier: MIT\n/**\n * Created on 2020-10-23\n * @summary: JLoanEthDeployer Interface\n * @author: Jibrel Team\n */\npragma solidity 0.6.11;\n\n\ninterface IJLoanDeployer {\n    function setLoanFactory(address _factAddr) external;\n    function setLoansCounter(uint _newValue) external;\n    function getDeployedLoansCounter() external view returns(uint);\n    function getDeployedLoanAddress(uint idx) external view returns(address);\n    function isLoanDeployed(address _loanAddr) external view returns (bool);\n    function deployNewLoanContract(uint _pairId, address _factAddr, address _collToken, address _lendToken/*, bool _isEth*/) external payable returns (address);\n}\n",
